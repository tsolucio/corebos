<tables>
<table>
<name>vtiger_cbprocessinfo</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocessinfo` (
  `cbprocessinfoid` int(11) NOT NULL,
  `cbprocessinfo_no` varchar(56) default NULL,
  `infodesc` varchar(110) default null,
  `active` varchar(3) default null,
  `semodule` varchar(110) default null,
  `fieldmap` int(11) default null,
  `valmap` int(11) default null,
  `depmap` int(11) default null,
  PRIMARY KEY (`cbprocessinfoid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
