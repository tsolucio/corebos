  <tables>
    <table>
      <name>vtiger_businessactions</name>
      <sql><![CDATA[CREATE TABLE `vtiger_businessactions` (
  `businessactionsid` int(11) NOT NULL,
  `elementtype_action` varchar(250) default NULL,
  `linklabel` varchar(250) default NULL,
  `linkurl` text default NULL,
  `sequence` int(25) default NULL,
  `handler_path` varchar(250) default NULL,
  `onlyonmymodule` varchar(25) default NULL,
  `handler_class` varchar(250) default NULL,
  `module_list` varchar(250) default NULL,
  `linkicon` varchar(250) default NULL,
  `handler` varchar(250) default NULL,
  `active` varchar(250) default NULL,
  `acrole` varchar(250) default NULL,
  `mandatory` varchar(250) default NULL,
  `brmap` int(11) default NULL,
  `businessactions_no` varchar(56) default NULL,
  PRIMARY KEY (`businessactionsid`),
  index (`elementtype_action`),
  index (`module_list`),
  index (`active`),
  index (`acrole`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
    </table>
    <table>
