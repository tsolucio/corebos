<tables>
<table>
<name>vtiger_cbprocessstep</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocessstep` (
  `cbprocessstepid` int(11) NOT NULL,
  `cbprocessstep_no` varchar(65) default NULL,
  `processflow` int(11) default NULL,
  `fromstep` varchar(155) DEFAULT NULL,
  `tostep` varchar(155) DEFAULT NULL,
  `active` varchar(3) DEFAULT NULL,
  `validation` int(11) DEFAULT NULL,
  `context` int(11) DEFAULT NULL,
  `isactivevalidation` int(11) DEFAULT NULL,
  `showstepvalidation` int(11) DEFAULT NULL,
  `defaultpath` varchar(3) DEFAULT NULL,
  `validdefaultstart` varchar(3) DEFAULT NULL,
  PRIMARY KEY (`cbprocessstepid`),
  INDEX(`processflow`,`fromstep`,`tostep`,`active`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbprocessstepcf</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocessstepcf` (
  `cbprocessstepid` int(11) NOT NULL,
  PRIMARY KEY (`cbprocessstepid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbprocesssteprel</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocesssteprel` (
  `stepid` int(11) NOT NULL,
  `wfid` int(11) NOT NULL,
  `positive` tinyint(1) NOT NULL,
  PRIMARY KEY (`stepid`,`wfid`,`positive`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
