<tables>
<table>
<name>vtiger_cbprocessflow</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocessflow` (
  `cbprocessflowid` int(11) NOT NULL,
  `cbprocessflow_no` varchar(65) default NULL,
  `processflowname` varchar(255) default NULL,
  `pfmodule` varchar(155) DEFAULT NULL,
  `pffield` varchar(155) DEFAULT NULL,
  `active` varchar(3) DEFAULT NULL,
  `pfcondition` int(11) DEFAULT NULL,
  `pfinitialstates` varchar(211) DEFAULT NULL,
  PRIMARY KEY (`cbprocessflowid`),
  INDEX(`pfmodule`,`active`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbprocessflowcf</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbprocessflowcf` (
  `cbprocessflowid` int(11) NOT NULL,
  PRIMARY KEY (`cbprocessflowid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
