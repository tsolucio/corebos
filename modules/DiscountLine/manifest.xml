<tables>
<table>
<name>vtiger_discountline</name>
<sql><![CDATA[CREATE TABLE `vtiger_discountline` (
  `discountlineid` int(11) NOT NULL DEFAULT '0',
  `discountline_no` varchar(100) DEFAULT NULL,
  `dlcategory` varchar(255) DEFAULT NULL,
  `discount` int(11) DEFAULT NULL,
  `activestatus` varchar(3) DEFAULT 0,
  `returnvalue` varchar(50), 
  `cbmapid` int(11),
  PRIMARY KEY (`discountlineid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_discountlinecf</name>
<sql><![CDATA[CREATE TABLE `vtiger_discountlinecf` (
  `discountlineid` int(11) NOT NULL,
  PRIMARY KEY (`discountlineid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
